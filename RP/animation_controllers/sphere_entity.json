{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.sphere_entity.drip": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"dripping": "query.is_powered"
						}
					]
				},
				"dripping": {
					"particle_effects": [
						{
							"effect": "nectar_dripping"
						}
					],
					"transitions": [
						{
							"default": "!query.is_powered"
						}
					]
				}
			}
		},
		"controller.animation.sphere_entity.sting": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						"sphere_entity_sting"
					]
				}
			}
		},
		"controller.animation.sphere_entity.root": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						"flying",
						"drip",
						"sphere_entity_fly_bobbing",
						"controller_sphere_entity_sting"
					]
				}
			}
		}
	}
}